@keyframes hero-caption
  0% {
    opacity 0
    transform scale(0)
  }
  50% {
    opacity 1
    transform scale(1)
  }
  100% {
    transform none
  }
@keyframes fade-slide-down
  0% {
    opacity 0
    transform translateY(-4rem)
  }
  100% {
    opacity 1
    transform none
  }
@keyframes slide-left
  0% {
    transform none
  }
  100% {
    transform translateX(-100%)
  }
@keyframes slide-right
  0% {
    transform none
  }
  100% {
    transform translateX(100%)
  }
@keyframes fade-in-slide-right
  0% {
    opacity 1
    transform translateX(-100%)
  }
  100% {
    opacity 1
    transform none
  }
@keyframes slide-left
  0% {
    opacity 1
    transform none
  }
  100% {
    opacity 1
    transform translateX(-100%)
  }
@keyframes fade-in-slide-left
  0% {
    opacity 1
    transform translateX(100%)
  }
  100% {
    opacity 1
    transform none
  }
@keyframes fade-slide-up
  0% {
    opacity 0
    transform translateY(4rem)
  }
  100% {
    opacity 1
    transform none
  }
@keyframes splash
  0% {
    opacity 0
    transform scale(0) translateY(-4rem)
  }
  100% {
    opacity 1
    transform none
  }
@keyframes list-group-item-show
  0% {
    display none
    opacity 0
    transform rotateY(90deg)
  }
  100% {
    display block
    opacity 1
    transform none
  }
@keyframes spinner{
  0% {
    transform: rotate(0deg);
  }
  100% {
    transform: rotate(360deg);
  }
}

@keyframes beat{
  0% {
    transform: scale(0.95);
  }
  5% {
    transform: scale(1.1);
  }
  39% {
    transform: scale(0.85);
  }
  45% {
    transform: scale(1);
  }
  60% {
    transform: scale(0.95);
  }
  100% {
    transform: scale(0.9);
  }
}

@keyframes spinner3d {
  0%, 100% {
    animation-timing-function: cubic-bezier(0.5, 0, 1, 0.5);
  }
  0% {
    transform: rotateY(0deg);
  }
  50% {
    transform: rotateY(1800deg);
    animation-timing-function: cubic-bezier(0, 0.5, 0.5, 1);
  }
  100% {
    transform: rotateY(3600deg);
  }
}
@keyframes pulse{
  0% {
    top: 28px;
    left: 28px;
    width: 0;
    height: 0;
    opacity: 1;
  }
  100% {
    top: -1px;
    left: -1px;
    width: 58px;
    height: 58px;
    opacity: 0;
  }
}